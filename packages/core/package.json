{
  "name": "@lonestone/nzoth",
  "version": "0.0.1",
  "private": false,
  "description": "NZOTH: Nest + Zod + OpenAPI + Typed + Helpers - Build fully type-safe REST APIs with NestJS",
  "author": "Lonestone",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lonestone/nzoth.git"
  },
  "bugs": {
    "url": "https://github.com/lonestone/nzoth/issues"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nestjs",
    "rest",
    "api",
    "zod",
    "validation",
    "openapi",
    "swagger",
    "typescript"
  ],
  "exports": {
    "./client": {
      "types": "./dist/client.d.ts",
      "default": "./dist/client.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "default": "./dist/server.js"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm build && pnpm test"
  },
  "dependencies": {
    "@anatine/zod-openapi": "^2.2.7",
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/swagger": "^11.0.1",
    "nestjs-zod": "^4.3.1",
    "rxjs": "^7.8.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.17.24",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^1.6.1",
    "supertest": "^7.0.0",
    "typescript": "5.4.2",
    "vitest": "^1.6.1"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
